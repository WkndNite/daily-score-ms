<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.StudentMapper">
    <insert id="insertStudentByXML" parameterType="map">
        INSERT INTO ${tableName} (id, name, <foreach collection="names" item="name" separator=",">${name}</foreach>)
        VALUES (#{student.id}, #{student.name}, <foreach collection="names" item="name" separator=",">#{student.${name}}</foreach>)
    </insert>
</mapper>